{
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["ES2021", "DOM"],
    "jsx": "react",
    "moduleResolution": "node",
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "*": ["*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
